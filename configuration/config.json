{
  "LogAnalyzer": {
    "EnableDebug": false,
    "Filters": [
      {
        "Name": "Info",
        "Regex": "(?m)^.*\\[.*INFO\\].*"
      },
      {
        "Name": "Error",
        "Regex": "(?m)^.*\\[.*ERROR\\].*"
      },
      {
        "Name": "JavaStackTrace",
        "Regex": "(?m)^.*?Exception.*(?:[\\r|\\n]+^\\s*at .*)+"
      },
      {
        "Name": "StackTrace",
        "Regex": "(?m)((.*(\\n|\\r|\\r\\n)){1})^.*?Exception.*(?:[\\n|\\r|\\r\\n]+^\\s*at .*)+"
      }
    ]
  }
}